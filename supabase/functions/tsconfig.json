{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "nodenext",
        "lib": [
            "ESNext"
        ],
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "./**/*.ts"
    ]
}